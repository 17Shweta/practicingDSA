
//rotate matrix by 90 degree clockwise

//rotate clockwise means top row becomes right column


#include <stdio.h>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n=4;
 int arr[n][n];
 for(int i=0;i<n;i++)
 {
  for(int j=0;j<n;j++)
   {
     cin>>arr[i][j];
   }   
 }
 
 for(int i=0;i<n;i++)
 {
  for(int j=n-1;j>=0;j--)
   {
     cout<<arr[j][i];
   }
   cout<<endl;
 }
 
}

output: 
1 2 3 4
9 8 5 6
6 5 3 7
9 2 6 8

9691
2582
6353
8764


